<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
	
<configuration>
	<!-- 별칭 등록 -->
	<typeAliases>
		<typeAlias 
			type="himedia.photobook.repositories.vo.UsersVo"
			alias="UsersVo" />
		<typeAlias type="himedia.photobook.repositories.vo.uBoardVo"
			alias="uBoardVo" />
	</typeAliases>
	
	<!-- MyBatis 매퍼 -->
	<mappers> 
		<mapper resource="mybatis/mappers/users.xml" />
		<mapper resource="mybatis/mappers/profile.xml" />
		<mapper resource="mybatis/mappers/uboard.xml"/>
	</mappers>
	
	
</configuration>